{
    "Builds": [
        {
            "src": "projeto/wsgi.py",

            "use": "@vercel/python",

            "config": {
                "maxiLambaSize": "15mb",

                "runtime": "python 3.9.9"

            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "projeto/wsgi.py"

        }

    ]

 }
